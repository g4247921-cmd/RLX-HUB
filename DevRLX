-- Services
local Players      = game:GetService("Players")
local LocalPlayer  = Players.LocalPlayer
local PlayerGui    = LocalPlayer:WaitForChild("PlayerGui")
local TweenService = game:GetService("TweenService")
local UIS          = game:GetService("UserInputService")
local RunService   = game:GetService("RunService")
local HttpService  = game:GetService("HttpService")
local Debris       = game:GetService("Debris")

-- Defensive remove previous
if PlayerGui:FindFirstChild("RLX_V22") then
	pcall(function() PlayerGui.RLX_V22:Destroy() end)
end

-- safeLoad helper (returns ok, err)
local function safeLoad(url)
	if not url or url == "" then return false, "empty url" end
	local ok, res = pcall(function() return game:HttpGet(url) end)
	if not ok then return false, res end
	local fn, err = loadstring(res)
	if not fn then return false, err end
	local ok2, err2 = pcall(fn)
	return ok2, err2
end

-- tween helper
local function tween(inst, props, t)
	local info = TweenInfo.new(t or 0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local suc, tw = pcall(function() return TweenService:Create(inst, info, props) end)
	if suc and tw then tw:Play() end
	return tw
end

