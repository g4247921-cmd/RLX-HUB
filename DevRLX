-- sounds (soft)
local function makeSound(parent, id, vol)
	local s = Instance.new("Sound", parent)
	s.SoundId = "rbxassetid://"..tostring(id)
	s.Volume = vol or 1
	return s
end
local ClickSound = makeSound(ScreenGui, 9118823101, 0.9)
local OpenSound  = makeSound(ScreenGui, 4590657391, 0.8)
local CloseSound = makeSound(ScreenGui, 9118823107, 0.7)
local function Click() pcall(function() ClickSound:Play() end) end

