local function setActive(tab)
	for _,c in pairs(Sidebar:GetChildren()) do
		if c:IsA("TextButton") then c.BackgroundColor3 = Color3.fromRGB(120,85,20) end
	end
	if tab and tab:IsA("TextButton") then tab.BackgroundColor3 = Color3.fromRGB(200,150,40) end
end

-- ========== Content & Pages (each page is ScrollingFrame) ==========
local Content = Instance.new("Frame", Main)
Content.Size = UDim2.new(0.74,0,0.84,0)
Content.Position = UDim2.new(0.26,0,0.14,0)
Content.BackgroundTransparency = 1

local Pages = {}
local function NewPage(name)
	local p = Instance.new("ScrollingFrame", Content)
	p.Name = name
	p.Size = UDim2.new(1,0,1,0)
	p.CanvasSize = UDim2.new(0,0,0,0)
	p.ScrollBarThickness = 6
	p.BackgroundTransparency = 1
	p.Visible = false
	p.AutomaticCanvasSize = Enum.AutomaticSize.Y

	local layout = Instance.new("UIListLayout", p)
	layout.Padding = UDim.new(0,8)
	layout.SortOrder = Enum.SortOrder.LayoutOrder
	layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		local y = layout.AbsoluteContentSize.Y + 16
		p.CanvasSize = UDim2.new(0,0,0,y)
	end)
	return p
end

Pages.Home   = NewPage("Home")
Pages.Rules  = NewPage("Rules")
Pages.LT2    = NewPage("LT2")
Pages.Fish   = NewPage("Fish")
Pages.Scripts= NewPage("Scripts")
Pages.Credits= NewPage("Credits")

local function ShowPage(name)
	for k,v in pairs(Pages) do v.Visible = (k == name) end
end
ShowPage("Home"); setActive(TabHome)

-- Tab connections
