	for i=1,18 do
		Logo.ImageTransparency = math.clamp(Logo.ImageTransparency - 0.05, 0, 1)
		task.wait(0.02)
	end
	PopText.Text = "RLX HUB"
	for i=1,12 do PopText.TextTransparency = 1 - (i/12); task.wait(0.02) end
	task.wait(0.2)
	PopText.Text = "Respect â€¢ Loyalty â€¢ X-Factor âš¡"
	for i=1,12 do PopText.TextTransparency = 1 - (i/12); task.wait(0.02) end
	task.wait(0.45)
	for i=1,20 do
		Intro.BackgroundTransparency = i/20
		Logo.ImageTransparency = math.clamp(Logo.ImageTransparency + 0.05, 0, 1)
		PopText.TextTransparency = math.clamp(PopText.TextTransparency + 0.05, 0, 1)
		task.wait(0.02)
	end
	if Intro and Intro.Parent then Intro:Destroy() end
end)

