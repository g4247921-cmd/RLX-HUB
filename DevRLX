-- toggle function
local function toggleMain(vis)
	Click()
	if vis then pcall(function() OpenSound:Play() end) end
	if not vis then pcall(function() CloseSound:Play() end) end
	Main.Visible = vis
end
ToggleOpen.MouseButton1Click:Connect(function() toggleMain(not Main.Visible) end)
BtnClose.MouseButton1Click:Connect(function() toggleMain(false) end)
BtnMin.MouseButton1Click:Connect(function() toggleMain(false) end)

-- draggable main (mouse+touch)
do
	local dragging = false
	local dragStart = Vector2.new()
	local startPos = Main.Position
	local function onBegin(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = Main.Position
		end
	end
	local function onEnd(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = false
		end
	end
	local function onChange(input)
		if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
			local delta = input.Position - dragStart
			Main.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end
	Top.InputBegan:Connect(onBegin)
	Top.InputEnded:Connect(onEnd)
	UIS.InputChanged:Connect(onChange)
end

